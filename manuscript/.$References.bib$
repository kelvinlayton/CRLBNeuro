@CONFERENCE{Bose2001,
  author = {Bose, N K and Lertrattanapanich, S and Koo, J},
  title = {{Advances in superresolution using L-curve}},
  booktitle = {Proceedings - IEEE International Symposium on Circuits and Systems},
  year = {2001},
  volume = {2},
  pages = {II433--II436},
  abstract = {Subsequent to the work of Kim, Bose, and Valenzuela in 1990 on the
	simultaneous filtering and interpolation of a registered sequence
	of undersampled noisy and shift-invariant blur degraded images, Bose
	and Boo tackled in 1998 the problem of reconstructing a high-resolution
	image from multiple undersampled, shifted, degraded frames with subpixel
	displacement errors. This led to a formulation involving a periodically
	shift-variant system model. Lertrattanapanich and Bose advanced in
	1999 a procedure for construction of a high-resolution video mosaic
	following the estimation of motion parameters between successive
	frames in a video sequence generated from a video camera. Current
	research is focused on simultaneous blur identification and robust
	superresolution. The blur is not restricted to be linear shift-invariant
	and could not only be of the linear shift-variant type but also some
	nonlinear blurs could be accommodated. The optimal tuning parameter
	may, if desired, be calculated analytically and not by trial-and-error.},
