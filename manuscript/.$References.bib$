@ARTICLE{Bendall1983,
  author = {Bendall, M Robin and Gordon, Roy E},
  title = {{Depth and refocusing pulses designed for multipulse NMR with surface
	coils}},
  journal = {Journal of Magnetic Resonance},
  year = {1983},
  volume = {53},
  pages = {365--385},
  number = {3},
  abstract = {When surface radiofrequency coils are used for irradiation and detection,
	the pulse angles are greatest in the plane of the coil and decrease
	rapidly with depth of penetration into the sample and this intrinsic
	spatial variation in pulse angle makes it difficult to use multipulse
	NMR techniques. However, it is shown that spin-echo pulse sequences
	may still be used provided the refocusing pulses are cycled through
	all four phases as described by Bodenhausen et al. (J. Magn. Reson.
	27, 511 (1977)). This has the added advantage that signals are markedly
	reduced from sample regions where there is a large deviation from
	an ideal pulse angle. Thus the scheme also functions as a depth pulse
	sequence, and this property is preserved even when there are no delay
	periods. Additional depth discrimination is obtained by using additional
	phase-cycled pulses, e.g., the depth pulseAnalysis of the phase alternation
	introduced by Demco et al. (J. Magn. Reson. 16, 467 (1974)) for inversion-recovery
	T1 measurements shows that when this is combined with the above cycled
	pulses, additional depth discrimination is obtained and accurate
	t1 measurements may be made even when there are large divergences
	from the ideal pulse angles. Thus is a very good depth pulse may
	be used for inversion-recovery T1, measurements and-may be used for
	spin-echo sequences. Several simple spin-echo sequences are presented.
	These techniques help to relax the constraints on radio-frequency
	coil design imposed by the requirement to have good B1, homogeneity
	and therefore may have more general application, especially in cases
	where the need for a large filling factor is at a premium.},
  doi = {10.1016/0022-2364(83)90211-1},
  file = {:Users/klayton/Dropbox/Library/Papers/Bendall, Gordon\_1983\_Depth and refocusing pulses designed for multipulse NMR with surface coils.pdf:pdf},
  issn = {0022-2364},
  url = {http://www.sciencedirect.com/science/article/pii/0022236483902111}
}

@ARTICLE{Benedetto2003,
  author = {Benedetto, J J and Heinig, H P},
  title = {{Weighted Fourier Inequalities: New Proofs and Generalizations}},
  journal = {Journal of Fourier Analysis and Applications},
  year = {2003},
  volume = {9},
  pages = {1--37},
  number = {1},
  abstract = {Fourier transform inequalities in weighted Lebesgue spaces are proved.The
	inequalities are generalizations of the Plancherel theorem, theyare
	characterized in terms of uncertainty principle relationsbetween
	pairs of weights, and they are put in the context of existingweighted
	Fourier transform inequalities. The proofs are new and relativelyelementary,
	and they give rise to good and explicit constants controllingthe
	continuity of the Fourier transform operator. The smaller the constantis,the
	more applicable the inequality will be in establishing weighteduncertainty
	principle or entropy inequalities. There are two essentiallydifferent
	proofs, one depending on operator theory and one depending onLorentz
	spaces. The results from these approaches are quantitativelycompared,
	leading to classical questions concerning multipliers and tonew questions
	concerning wavelets.},
  annote = {10.1007/s00041-003-0003-3 },
  file = {:Users/klayton/Dropbox/Library/Papers/Benedetto, Heinig\_2003\_Weighted Fourier Inequalities New Proofs and Generalizations.pdf:pdf},
  issn = {1069-5869},
  publisher = {Birkhuser Boston},
  url = {http://dx.doi.org/10.1007/s00041-003-0003-3}
}

