@ARTICLE{Louis1983,
  author = {Louis, A K and Natterer, F},
  title = {{Mathematical problems of computerized tomography}},
  journal = {Proceedings of the IEEE},
  year = {1983},
  volume = {71},
  pages = {379--389},
  number = {3},
  abstract = {The data measured in computerized tomography, e.g., the X-ray attenuation
	in X-ray tomography or the resonance phenomena in nuclear magnetic
	resonance tomography, have to be processed to produce the pictures
	on which the diagnostic evaluation of the physician is based. This
	process consists of the solution of the following mathematical problem.
	The data depend on the searched-for distribution and this dependence
	can be described as an integral transform. To produce the final picture
	amounts to the inversion of the integral transform. This paper is
	concerned with the description of the integral transforms modeling
	the different techniques in computerized tomography. Among other
	things, the following questions are treated. Which numerical problems
	do we have to encounter in inverting the transforms; e.g., what accuracy
	in the reconstruction can we expect in dependence on the accuracy
	of the data? To what extent is a distribution determined by a finite
	number of measurements? Is it possible to recover the distribution
	reliably if the data are incomplete? The data measured in computerized
	tomography; e. g, the X-ray attenuation in X-ray tomography or the
	resonance phenomena in nuclear magnetic resonance tomography have
	to be processed to produce the magnetic resonance tomography, have
	to be processed to produce the pictures on which the diagnostic evaluation
	of the physician is based. This process consists of the solution
	of the following mathematical problem. The data depend on the searched-for
	distribution and this dependence can be described as an integral
	transform. To produce the final picture amounts to the inversion
	of the integral transform. The integral transform modeling to different
	techniques in Ct are described and various questions and problems
	are treated.},
  file = {:Users/klayton/Dropbox/Library/Papers/Louis, Natterer\_1983\_Mathematical problems of computerized tomography.pdf:pdf},
