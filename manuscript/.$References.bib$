@ARTICLE{Haacke1987,
  author = {Haacke, E M},
  title = {{The effects of finite sampling in spin-echo or field-echo magnetic
	resonance imaging}},
  journal = {Magnetic Resonance in Medicine},
  year = {1987},
  volume = {4},
  pages = {407--421},
  number = {5},
  abstract = {The artifacts associated with finite sampling in magnetic resonance
	imaging are shown to be significant when the ratio of half the sampling
	time, Ts/2, to transverse relaxation time, T2 is on the order of
	unity or greater. For both symmetric and asymmetric sampling, these
	artifacts include enhanced high spatial frequency image intensity
	(bright edges), induced systematic noise, loss of resolution, and
	changes in phase. Simple filtering of the raw data does not remove
	the problem in tissues with positiondependent T2 values. An exact
	deconvolution method is described to remove these problems associated
	with finite position-dependent T2 values. 1987 Academic Press, Inc.},
  doi = {10.1002/mrm.1910040502},
  file = {:Users/klayton/Dropbox/Library/Papers/Haacke\_1987\_The effects of finite sampling in spin-echo or field-echo magnetic resonance imaging.pdf:pdf},
  issn = {1522-2594},
