@ARTICLE{Stoica2001,
  author = {Stoica, P and Marzetta, T L},
  title = {{Parameter estimation problems with singular information matrices}},
  journal = {Signal Processing, IEEE Transactions on},
  year = {2001},
  volume = {49},
  pages = {87--90},
  number = {1},
  abstract = {The case of a singular Fisher information matrix (FIM) represents
	a significant complication for the theory of the Cramer-Rao lower
	bound (CRB) that is usually handled by resorting to the pseudoinverse
	of the Fisher matrix. We take a different approach in which the CRB
	is derived as the solution to an unconstrained quadratic maximization
	problem, which enables us to handle the singular case in a simple
	yet rigorous manner. When the Fisher matrix is singular, except under
	unusual circumstances, any estimator having the specified bias derivatives
	that figure in the CRB must have infinite variance},
