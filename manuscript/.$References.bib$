@ARTICLE{Zibulski1997,
  author = {Zibulski, M and Zeevi, Y Y},
  title = {{Analysis of Multiwindow Gabor-Type Schemes by Frame Methods}},
  journal = {Applied and Computational Harmonic Analysis},
  year = {1997},
  volume = {4},
  pages = {188--221},
  number = {2},
  abstract = {The Gabor scheme is generalized to incorporate several window functions
	as well as kernels other than the exponential. The properties of
	the sequence of representation functions are characterized by an
	approach based on the concept of frames. Utilizing the piecewise
	Zak transform (PZT), the frame operator associated with the multiwindow
	Gabor-type frame is examined for a rational oversampling rate by
	representing the frame operator as a finite-order matrix-valued function
	in the PZT domain. Completeness and frame properties of the sequence
	of representation functions are examined in relation to the properties
	of the matrix-valued function. Calculations of the frame bounds and
	the dual frame, as well as the issue of tight frames, are considered.
	It is shown that the properties of the sequence of representation
	functions are essentially not changed by replacing the widely used
	exponential kernel with other kernels. Some examples and the issue
	of a different sampling rate for each window are also considered.
	The so-called Balian-Low theorem is generalized to consideration
	of a scheme of multiwindows which makes it possible to overcome in
	a way the constraint imposed by the original theorem in the case
	of a single Window. 1997 Academic Press.},
  file = {:Users/klayton/Dropbox/Library/Papers/Zibulski, Zeevi\_1997\_Analysis of Multiwindow Gabor-Type Schemes by Frame Methods.pdf:pdf},
  url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-0000861229\&partnerID=40\&md5=89e31386f2923b3a718e3bb5b879de72}
}

@ARTICLE{Zientara1999,
  author = {Zientara, G P and Panych, L P and Jolesz, F A},
  title = {{Near-Optimal Spatial Encoding for Dynamically Adaptive \{MRI\}:
	Mathematical Principles and Computational Methods}},
  journal = {International Journal of Imaging Systems and Technology},
  year = {1999},
  volume = {10},
  pages = {151--165},
  number = {2},
  abstract = {The mathematical principles of near-optimal two-dimensional spatial
	encoding for dynamically adaptive magnetic resonance imaging (MRI)
	are presented together with a survey of numerical methods applicable
	for the computation of these encodes. Two main classes of linear
	algebraic techniques are identified - rank revealing orthogonal decompositions
	and Krylov subspace methods - that are specially suited for the determination
	of efficient adaptive non-Fourier spatial encoding. Simulation results
	are presented to demonstrate usage. The key property of these methods
	is their ability to compute reduced vector basis sets, used as encoding
	profiles, that span the column and row vectcr subspaces of an image
	estimate array. Efficient encoding in MRI is possible since MR images
	typically represent rank deficient matrices. New methods for computing
	hybrid encodings based on near-optimal encodes are also described.
	An analysis of the applicability and efficiency of near-optimal encoding
	is presented using the principal angles between vector basis sets
	of the theoretical ideal image and that from an image estimate actually
	used for acquisition. 1999 John Wiley \& Sons, Inc.},
  file = {:Users/klayton/Dropbox/Library/Papers/Zientara, Panych, Jolesz\_1999\_Near-Optimal Spatial Encoding for Dynamically Adaptive \{MRI\} Mathematical Principles and Computational Methods.pdf:pdf},
  keywords = { Krylov subspace methods, Magnetic resonance imaging,Computational
	methods; Computer simulation; Image },
  url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-0032688188\&partnerID=40\&md5=30a26c90f14fea3db8a555af6ba887ec}
}

