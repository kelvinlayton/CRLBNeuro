@ARTICLE{Stanisz2005,
  author = {Stanisz, G J and Odrobina, E E and Pun, J and Escaravage, M and Graham,
	S J and Bronskill, M J and Henkelman, R M},
  title = {{T1, T2 relaxation and magnetization transfer in tissue at 3T}},
  journal = {Magnetic Resonance in Medicine},
  year = {2005},
  volume = {54},
  pages = {507--512},
  number = {3},
  abstract = {T-1, T-2, and magnetization transfer (MT) measurements were performed
	in vitro at 3 T and 37 degrees C on a variety of tissues: mouse liver,
	muscle, and heart; rat spinal cord and kidney; bovine optic nerve,
	cartilage, and white and gray matter; and human blood. The MR parameters
	were compared to those at 1.5 T. As expected, the T2 relaxation time
	constants and quantitative MT parameters (MT exchange rate, R, macromolecular
	pool fraction, M-0B, and macromolecular T-2 relaxation time, T-2B)
	at 3 T were similar to those at 1.5 T. The T, relaxation time values,
	however, for all measured tissues increased significantly with field
	strength. Consequently, the phenomenological MT parameter, magnetization
	transfer ratio, MTR, was lower by approximately 2 to 10\%. Collectively,
	these results provide a useful reference for optimization of pulse
	sequence parameters for MRI at 3 T.},
  doi = {10.1002/mrm.20605},
  file = {:Users/klayton/Dropbox/Library/Papers/Stanisz et al.\_2005\_T1, T2 relaxation and magnetization transfer in tissue at 3T.pdf:pdf},
  issn = {0740-3194},
  keywords = {3 T,MTR,T-1,T-2,liver,magnetization transfer}
