@ARTICLE{Yuan2012,
  author = {Yuan, Jing and Li, Yujia and Zhao, Feng and Chan, Queenie and Ahuja,
	Anil T and Wang, Yi-Xiang J},
  title = {{Quantification of T 1relaxation by using rotary echo spin-lock pulses
	in the presence of B 0 inhomogeneity}},
  journal = {Physics in Medicine and Biology},
  year = {2012},
  volume = {57},
  pages = {5003},
  number = {15},
  abstract = {T 1relaxation is traditionally described as a mono-exponential signal
	decay with spin-lock time. However, T 1quantification by fitting
	to the mono-exponential model can be substantially compromised in
	the presence of field inhomogeneities, especially for low spin-lock
	frequencies. The normal approach to address this issue involves the
	development of dedicated composite spin-lock pulses for artifact
	reduction while still using the mono-exponential model for T 1fitting.
	In this work, we propose an alternative approach for improved T 1quantification
	with the widely-used rotary echo spin-lock pulses in the presence
	of B 0inhomogeneities by fitting to a modified theoretical model
	which is derived to reveal the dependence of T 1 -prepared magnetization
	on T 1 , T 2 , spin-lock time, spin-lock frequency and off-resonance,
	without involving complicated spin-lock pulse design. It has potentials
	for T 1quantification improvement at low spin-lock frequencies. Improved
	T 1mapping was demonstrated on phantom and in vivo rat spin-lock
	imaging at 3 T compared to the mapping using the mono-exponential
	model.},
  file = {:Users/klayton/Dropbox/Library/Papers/Yuan et al.\_2012\_Quantification of T 1relaxation by using rotary echo spin-lock pulses in the presence of B 0 inhomogeneity.pdf:pdf},
  url = {http://stacks.iop.org/0031-9155/57/i=15/a=5003}
}

@ARTICLE{Zahneisen2011,
  author = {Zahneisen, B and Grotz, T and Lee, K J and Ohlendorf, S and Reisert,
	M and Zaitsev, M and Hennig, J},
  title = {{Three-dimensional MR-encephalography: Fast volumetric brain imaging
	using rosette trajectories}},
  journal = {Magnetic Resonance in Medicine},
  year = {2011},
  volume = {65},
  pages = {1260--1268},
  number = {5},
  abstract = {MR-Encephalography (MREG) is a technique that allows real time observation
	of functional changes in the brain that appears within 100 msec.
	The high sampling rate is achieved at the cost of some spatial resolution.
	The article describes a novel imaging method for fast three-dimensional-MR-encephalography
	whole brain coverage based on rosette trajectories and the use of
	multiple small receiver coils. The technique allows the observation
	of changes in brain physiology at very high temporal resolution.
	A highly undersampled three-dimensional rosette trajectory is chosen,
	to perform single shot acquisition of k-space data within 23 msec.
	By using a 32-channel head coil array and regularized nonuniform
	Fourier transformation reconstruction, the spatial resolution is
	sufficient to detect even subtle centers of activation (e.g. human
	MT+). The method was applied to visual block design paradigms and
	compared with echo planar imaging-based functional MRI. As a proof-of-principle
	of the method's ability to detect local differences in the hemodynamic
	response functions, the analyzed MR-encephalography data revealed
	a spatially dependent delay of the arrival of the blood oxygenation
	level dependent response within the visual cortex. Magn Reson Med,
	2011. 2011 Wiley-Liss, Inc.},
  doi = {10.1002/mrm.22711},
  issn = {1522-2594},
  keywords = { FMRI, Rosette, fast functional imaging, volume imaging,MREG},
  publisher = {Wiley Subscription Services, Inc., A Wiley Company},
  url = {http://dx.doi.org/10.1002/mrm.22711}
}

