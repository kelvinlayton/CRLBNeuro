@ARTICLE{Brown1982,
  author = {Brown, T R and Kincaid, B M and Ugurbil, K},
  title = {{NMR chemical shift imaging in three dimensions}},
  journal = {Proceedings of the National Academy of Sciences},
  year = {1982},
  volume = {79},
  pages = {3523--3526},
  number = {11},
  abstract = {A method for obtaining the three-dimensional distribution of chemical
	shifts in a spatially inhomogeneous sample using Fourier transform
	NMR is presented. The method uses a sequence of pulsed field gradients
	to measure the Fourier transform of the desired distribution on a
	rectangular grid in (k,t) space. Simple Fourier inversion then recovers
	the original distribution. An estimated signal/noise ratio of 20
	in 10 min is obtained for an "image" of the distribution of a 10
	mM phosphorylated metabolite in the human head at a field of 20 kG
	with 2-cm resolution.},
