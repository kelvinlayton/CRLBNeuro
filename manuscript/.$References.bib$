@ARTICLE{Nelder1965,
  author = {Nelder, J A and Mead, R},
  title = {{A simplex method for function minimization}},
  journal = {The computer journal},
  year = {1965},
  volume = {7},
  pages = {308},
  number = {4},
  abstract = {A method is described for the minimization of a function of n variables,
	which depends on the
	
	comparison of function values at the (n 4- 1) vertices of a general
	simplex, followed by the
	
	replacement of the vertex with the highest value by another point.
	The simplex adapts itself to
	
	the local landscape, and contracts on to the final minimum. The method
	is shown to be effective
	
	and computationally compact. A procedure is given for the estimation
	of the Hessian matrix in
	
	the neighbourhood of the minimum, needed in statistical estimation
	problems},
  file = {:Users/klayton/Dropbox/Library/Papers/Nelder, Mead\_1965\_A simplex method for function minimization.pdf:pdf},
  publisher = {Br Computer Soc}
}

@ARTICLE{Ng2002a,
  author = {Ng, M K and Bose, N K},
  title = {{Analysis of displacement errors in high-resolution image reconstruction
	with multisensors}},
  journal = {IEEE Transactions on Circuits and Systems I: Fundamental Theory and
	Applications},
  year = {2002},
  volume = {49},
  pages = {806--813},
  number = {6},
  abstract = {An image-acquisition system composed of an array of sensors, where
	each sensor has a subarray of sensing elements of suitable size,
	has recently been popular for increasing the spatial resolution with
	high signal-to-noise ratio beyond the performance bound of technologies
	that constrain the manufacture of imaging devices. Small perturbations
	around the ideal subpixel locations of the sensing elements (responsible
	for capturing the sequence of undersampled degraded frames), because
	of imperfections in fabrication, limit the performance of the signal-processing
	algorithms for processing and integrating the acquired images for
	the desired enhanced resolution and quality. The contributions of
	this paper include an analysis of the displacement errors on the
	convergence rate of the iterative approach for solving the transform
	based preconditioned system of equations. Subsequently, it is established
	that the use of the MAP, L<sub>2</sub> norm or H<sub>1</sub> norm
	regularization functional leads to a proof of linear convergence
	of the conjugate gradient method in terms of the displacement errors
	caused by the imperfect subpixel locations. Results of simulation
	support the analytical results.},
  file = {:Users/klayton/Dropbox/Library/Papers/Ng, Bose\_2002\_Analysis of displacement errors in high-resolution image reconstruction with multisensors.pdf:pdf},
  keywords = { Conjugate gradient method; Displacement errors; H, Image reconstruction,Algorithms;
	Computer simulation; Convergence of nu},
