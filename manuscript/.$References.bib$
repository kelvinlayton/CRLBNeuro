@ARTICLE{Chaumette2008,
  author = {Chaumette, E and Galy, J and Quinlan, A and Larzabal, P},
  title = {{A New Barankin Bound Approximation for the Prediction of the Threshold
	Region Performance of Maximum Likelihood Estimators}},
  journal = {IEEE Transactions on Signal Processing},
  year = {2008},
  volume = {56},
  pages = {5319--5333},
  number = {11},
  abstract = {AbstractIt is well known that the ML estimator exhibits a
	
	threshold effect, i.e., a rapid deterioration of estimation accuracy
	
	below a certain signal-to-noise ratio (SNR) or number of
	
	snapshots. This effect is caused by outliers and is not captured
	
	by standard tools such as the Cramr-Rao bound (CRB). The
	
	search of the SNR threshold value (where the CRB becomes unreliable
	
	for prediction of maximum likelihood estimator variance)
	
	can be achieved with the help of the Barankin bound (BB), as
	
	proposed by many authors. The major drawback of the BB, in
	
	comparison with the CRB, is the absence of a general analytical
	
	formula, which compels one to resort to a discrete form, usually
	
	the McaulaySeidman bound (MSB), requesting the search of an
	
	optimum over a set of test points. In this paper, we propose a new
	
	practical BB discrete form that provides, for a given set of test
	
	points, an improved SNR threshold prediction in comparison with
	
	existing approximations (MSB, Abel bound, McaulayHofstetter
	
	bound) at the expense of the computational complexity increased
	
	by a factor, where is the number of unknown parameters.
	
	We have derived its expression for the general Gaussian
	
	observation model to be used in place of existing approximations},
  file = {:Users/klayton/Dropbox/Library/Papers/Chaumette et al.\_2008\_A New Barankin Bound Approximation for the Prediction of the Threshold Region Performance of Maximum Likelihood Estimators.pdf:pdf}
}

@ARTICLE{Chen2008,
  author = {Chen, G and Tang, J and Leng, S},
  title = {{Prior image constrained compressed sensing (PICCS): A method to
	accurately reconstruct dynamic CT images from highly undersampled
	projection data sets}},
  journal = {Medical Physics},
  year = {2008},
  volume = {35},
  pages = {660--663},
  number = {2},
  abstract = {When the number of projections does not satisfy the Shannon/Nyquist
	sampling requirement, streaking artifacts are inevitable in x-ray
	computed tomography (CT) images reconstructed using filtered backprojection
	algorithms. In this letter, the spatial-temporal correlations in
	dynamic CT imaging have been exploited to sparsify dynamic CT image
	sequences and the newly proposed compressed sensing (CS) reconstruction
	method is applied to reconstruct the target image sequences. A prior
	image reconstructed from the union of interleaved dynamical data
	sets is utilized to constrain the CS image reconstruction for the
	individual time frames. This method is referred to as prior image
	constrained compressed sensing (PICCS). In vivo experimental animal
	studies were conducted to validate the PICCS algorithm, and the results
	indicate that PICCS enables accurate reconstruction of dynamic CT
	images using about 20 view angles, which corresponds to an undersampling
	factor of 32. This undersampling factor implies a potential radiation
	dose reduction by a factor of 32 in myocardial CT perfusion imaging.
	2008 American Association of Physicists in Medicine.},
  annote = {cited By (since 1996) 20},
  file = {:Users/klayton/Dropbox/Library/Papers/Chen, Tang, Leng\_2008\_Prior image constrained compressed sensing (PICCS) A method to accurately reconstruct dynamic CT images from highly undersampled projection data sets.pdf:pdf},
  keywords = { Algorithms; Animals; Data Compression; Heart; Mal, Animalia, Computer-Assisted;
	Radiographic Image Enhancement, Computer-Assisted; Reproducibility
	of Results; Sa, Computer-Assisted; Swine; Tomography, X-Ray Computed,accuracy;
	algorithm; article; artifact; computer a}
}

@BOOK{Cheng1999,
