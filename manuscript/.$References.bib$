@INCOLLECTION{springerlink:10.1007/978-1-61737-992-5_4,
  author = {Deoni, Sean C L},
  title = {{Magnetic Resonance Relaxation and Quantitative Measurement in the
	Brain}},
  booktitle = {Magnetic Resonance Neuroimaging},
  publisher = {Humana Press},
  year = {2011},
  editor = {Modo, Michel and Bulte, Jeff W M and Walker, John M},
  volume = {711},
  series = {Methods in Molecular Biology},
  pages = {65--108},
  abstract = {Underlying the exquisite soft tissue contrast provided by magnetic
	resonance imaging are the inherent biophysical processes of relaxation.
	Through the intricate relationships between tissue microstructure
	and biochemistry and the longitudinal and transverse relaxation rates,
	quantitative measurement of these relaxation parameters is informative
	of tissue change associated with disease, neural plasticity, and
	other biological processes. Quantitative imaging studies can further
	facilitate more detailed characterizations of tissue, providing a
	more direct link between modern MR imaging and classic histochemical
	and histological studies. In this chapter, we briefly review the
	biophysical basis of relaxation, introducing and focusing specifically
	on the T 1 , T 2 , and T 2 * relaxation times and detail some of
	the more widely used and clinically feasible techniques for their
	in vivo measurement. Methods for analyzing relaxation data are covered,
	and a summary of significant results from reported neuroimaging studies
	is provided. Finally, the combination of relaxation time data with
	other quantitative imaging data, including diffusion tensor and magnetization
	transfer, is examined, with the aim of providing more thorough characterization
	of brain tissue.},
  annote = {10.1007/978-1-61737-992-5\_4},
  file = {:Users/klayton/Dropbox/Library/Papers/Deoni\_2011\_Magnetic Resonance Relaxation and Quantitative Measurement in the Brain.pdf:pdf},
  isbn = {978-1-61737-992-5},
  url = {http://dx.doi.org/10.1007/978-1-61737-992-5\_4}
}

